<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!--2  第一种方式 默认构造 -->
    <bean id="iOCOneService" class="study.chenji.spring.ioc.IOCOneServiceImpl">
        <property name="iocTwoService" ref="iOCTwoService"/>
    </bean>
    <bean id="iOCTwoService" class="study.chenji.spring.ioc.IOCTwoServiceImpl">
        <property name="iocOneService" ref="iOCOneService"></property>

    </bean>

</beans>